{
  "name": "wp-ghubpg-r",
  "version": "1.0.0",
  "description": "template for webpack build to react and github-pg deployment",
  "main": "index.js",
  "scripts": {
    "test": "npm run dev",
    "dev": "webpack serve --config ./webpack.config.js",
    "build": "NODE_ENV=production BABEL_ENV=node webpack -c ./build.webpack.config.js",
    "winBuild": "set \"NODE_ENV=production BABEL_ENV=node\" && webpack -c ./build.webpack.config.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "TheAnh",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "homepage": "http://theanh28.github.io/wp-ghubpg-r"
}
